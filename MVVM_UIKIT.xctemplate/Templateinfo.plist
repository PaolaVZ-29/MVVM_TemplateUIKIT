<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Template for creating a new MVVM model file.</string>

    <key>Identifier</key>
    <string>com.custom.mvvm.model</string>

    <key>Kind</key>
    <string>Xcode.IDEKit.TextSubstitutionFileTemplateKind</string>

    <key>Language</key>
    <string>swift</string>

    <key>Options</key>
    <array>
        <dict>
            <key>Identifier</key>
            <string>productName</string>
            <key>Label</key>
            <string>Product Name</string>
            <key>Type</key>
            <string>text</string>
            <key>Required</key>
            <true/>
        </dict>
    </array>

    <key>Platforms</key>
    <array>
        <string>com.apple.platform.iphonesimulator</string>
        <string>com.apple.platform.iphoneos</string>
    </array>

    <key>SortOrder</key>
    <string>1</string>

    <key>Summary</key>
    <string>Creates a new model for MVVM architecture.</string>

    <key>DefaultCompletionName</key>
    <string>NewModel</string>

    <key>Version</key>
    <integer>1</integer>

    <key>Definitions</key>
    <dict>
        <key>productName</key>
        <string>___VARIABLE_productName___</string>
    </dict>
